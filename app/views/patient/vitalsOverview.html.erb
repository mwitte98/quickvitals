<% provide(:title, "#{@patientName} Vitals") %>
<body>
	<div class="container">
		<h1><%= @patientName %> Vitals</h1>
		<% @vitals.each do |id, name| %>
			<%= link_to vital_path(@patientID, id) do %>
				<div class="well col-sm-6">
					<h4><%= name %></h4>
					<p>Insert graph here</p>
				</div>
			<% end %>
		<% end %>
	</div>
</body>